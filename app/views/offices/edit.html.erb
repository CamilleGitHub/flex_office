<h1>@office.name</h1>
<p>Find me in app/views/offices/edit.html.erb</p>


<%= simple_form_for(@office) do |f| %>
  <p><%= f.input :name %></p>
  <p><%= f.input :address %></p>
  <p><%= f.input :city %></p>
  <p><%= f.input :capacity %></p>
  <p><%= f.input :price %></p>
  <p><%= f.input :photo, as: :file %>
  <%= cl_image_tag @office.photo.key, height: 300, width: 400, crop: :fill %></p>
  <p><%= f.submit 'Edit', class: 'btn btn-primary'%></p>

<% end %>
